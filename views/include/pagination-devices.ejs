<div class="blog-pagenat-wthree">
  <ul>
    <% if (currentPage !== 1 && previousPage !== 1) { %>
        <li><a href="?device_type=<%= device_type%>&page=1">1</a></li>
      <% } %>
      <% if (hasPreviousPage) { %>
        <li>  <a href="?device_type=<%= device_type%>&page=<%= previousPage %>"><%= previousPage %></a></li>
      <% } %>
      <li><a href="?device_type=<%= device_type%>&page=<%= currentPage %>" class="last"><%= currentPage %></a> </li>
      <% if (hasNextPage) { %>
          <li><a href="?device_type=<%= device_type%>&page=<%= nextPage %>"><%= nextPage %></a> </li>
      <% } %>
      <% if (lastPage !== currentPage && nextPage !== lastPage) { %>
          <li><a href="?device_type=<%= device_type%>&page=<%= lastPage %>"><%= lastPage %></a> </li>
      <% } %>
  </ul>
</div>
